 this is a minimal lisp interpreter that runs directly on raspberry pi hardware without any operating system
 it provides a simple interactive repl over uart allowing you to type and evaluate Lisp expressions in real time

(+ 1 2)

(define x 10)

(+ x 5)

(cons 1 (cons 2 nil))

(car (cons 10 20))

(cdr (cons 10 20))

(if 0 1 2)

(if 1 1 2)

(quote (a b c))

(define y (cons 3 4))

(+ (car y) (cdr y))

(- 20 5)

(* 6 7)

(/ 20 4)

(define z 100)

(+ z (* 2 3))

(cons (quote a) nil)

(cdr (cons 1 (cons 2 nil)))

(if (quote nil) 1 2)

(if (quote t) 1 2)

(define lst (cons 1 (cons 2 (cons 3 nil))))

(car (cdr lst))

(define a 5)
(define b 10)
(+ a b)

(+ (* 2 3) (/ 10 5) (- 7 4))

(if 0 100 200)
(if 1 100 200)

(quote (1 2 3))

(cons (quote a) (quote b))

(define x 7)
(define y 3)
(+ (* x y) (- x y))
